# URL to connect to the database, typically includes driver, user, password, host, port, and database name.
DATABASE_URL=

# Secret key used for cryptographic operations like signing JWTs or securing sessions.
SECRET_KEY=

# Algorithm used for token signing, e.g., "HS256" for HMAC or "RS256" for RSA.
ALGORITHM=

# Token expiration time in minutes, defines how long access tokens are valid.
ACCESS_TOKEN_EXPIRE_MINUTES=

# OAuth 2.0 Client ID for Google integration, obtained from Google Developer Console.
GOOGLE_CLIENT_ID=

# OAuth 2.0 Client Secret for Google integration, used for authentication.
GOOGLE_CLIENT_SECRET=

# Redirect URI for Google OAuth 2.0, specifies where Google redirects after authentication.
GOOGLE_REDIRECT_URI=

# PostgreSQL username for database authentication.
POSTGRES_USER=

# PostgreSQL password for database authentication.
POSTGRES_PASSWORD=

# PostgreSQL database name to connect to.
POSTGRES_DB=

